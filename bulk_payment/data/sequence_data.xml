<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Bulk Payment sequences -->
        <record id="sequence_bulk_payment_customer_invoice" model="ir.sequence">
            <field name="name">Bulk Payments customer invoices sequence</field>
            <field name="code">account.bulk.payment.customer.invoice</field>
            <field name="prefix">BULK.CUST.IN/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_bulk_payment_customer_refund" model="ir.sequence">
            <field name="name">Bulk Payments customer credit notes sequence</field>
            <field name="code">account.bulk.payment.customer.refund</field>
            <field name="prefix">BULK.CUST.OUT/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_bulk_payment_supplier_invoice" model="ir.sequence">
            <field name="name">Bulk Payments supplier invoices sequence</field>
            <field name="code">account.bulk.payment.supplier.invoice</field>
            <field name="prefix">BULK.SUPP.OUT/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_bulk_payment_supplier_refund" model="ir.sequence">
            <field name="name">Bulk Payments supplier credit notes sequence</field>
            <field name="code">account.bulk.payment.supplier.refund</field>
            <field name="prefix">BULK.SUPP.IN/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>