<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--paper format for cheques-->
    <record id="cheque_printing_paperformat_bulk" model="report.paperformat">
        <field name="name">Cheque Printing Bulk</field>
        <field name="default" eval="False"/>
        <field name="format">A4</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">68</field>
        <field name="margin_bottom">28</field>
        <field name="margin_left">4</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">61</field>
        <field name="dpi">90</field>
    </record>
    <!--paper format for payment voucher-->
    <record id="payment_voucher_paperformat_bulk" model="report.paperformat">
        <field name="name">Payment Voucher Bulk</field>
        <field name="default" eval="False"/>
        <field name="format">A4</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">20</field>
        <field name="margin_bottom">5</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">15</field>
        <field name="dpi">80</field>
    </record>
    <!--paper format for payment receipt-->
    <record id="payment_receipt_paperformat_bulk" model="report.paperformat">
        <field name="name">Payment Receipt Bulk</field>
        <field name="default" eval="False"/>
        <field name="format">A4</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">20</field>
        <field name="margin_bottom">5</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">15</field>
        <field name="dpi">80</field>
    </record>
</odoo>